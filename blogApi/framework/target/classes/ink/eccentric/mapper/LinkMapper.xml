<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="ink.eccentric.mapper.LinkMapper">
    <!--
        1.查出的友链审核状态是通过的
    -->
    <select id="getAllLink" resultType="ink.eccentric.domain.vo.link.LinkVo">
        select * from link
        where status=0
          and del_flag=0
    </select>
</mapper>